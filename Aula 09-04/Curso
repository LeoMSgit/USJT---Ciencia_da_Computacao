package aula0904oo;

public class Curso {
    private int codigo;
    private String nome;
    private String periodo;
   
    public Curso(){
    this.codigo = 1515;
    this.nome = "Bora Bil da Silva";
    this.periodo = "Primeiro";
  }
    public void setCodigo(int codigo){
      this.codigo = codigo;
  }
    public void setNome(String ra){
      this.nome = nome;
  }
    public void setRa(String periodo){
      this.periodo = periodo;
  }
    public int getCodigo(){
      return this.codigo;
    }
    public String getNome(){
      return this.nome;
    }
    public String getPeriodo(){
      return this.periodo;
    }
      
    
}

